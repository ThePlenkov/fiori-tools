{
  "name": "ui5-ts",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/ui5-ts/src",
  "targets": {
    "version": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm version",
        "cwd": "packages/ui5-ts"
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm publish --access public",
        "cwd": "dist/packages/ui5-ts"
      },
      "dependsOn": [
        "build"
      ]
    }
  },
  "tags": []
}